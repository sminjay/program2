<div id="<%= dom_id show %>">
  <p>
    <strong>Movie:</strong>
    <%= show.movie.title %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= show.date %>
  </p>

  <p>
    <strong>Time:</strong>
    <%= show.time %>
  </p>

  <p>
    <strong>Available seats:</strong>
    <%= show.seats_available %>
  </p>

  <p>
    <strong>Ticket price:</strong>
    <%= show.ticket_price %>
  </p>

</div>
